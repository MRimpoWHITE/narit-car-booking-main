  nav {
    background-color: aliceblue;
  }

  nav .nvbr{
    display: flex;
    width: 83%;
    margin: auto;
    padding: 20px 0;
    align-items: center;
    justify-content: space-between;
}

  nav ul li{
    display: inline-block;
    list-style: none;
    margin: 0px 30px;
    padding-top: 15px;
    position: relative;
}

nav ul li::after{
    content: '';
    height: 3px;
    width: 0%;
    background: #ff6200e0;
    position: absolute;
    left: 0;
    bottom: -10px;
    transition: 0.5s;
}

nav ul li a{
    text-decoration: none;
    color: rgb(4, 0, 255);
    font-weight: bold;
} 

 nav ul li:hover::after{
    width: 100%;
 }



    </style>

